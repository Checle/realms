{
  "name": "jsvm",
  "version": "0.1.5",
  "description": "Pure ECMAScript 5 implementation of the Node.js VM API",
  "main": "dist/jsvm.js",
  "browser": "dist/browser.js",
  "scripts": {
    "prepublish": "npm run clean && typings install && npm run dist",
    "test": "ts-node node_modules/tape/bin/tape test.ts",
    "lint": "tslint *.ts",
    "build": "tsc",
    "dist": "npm run build && webpack dist/jsvm.js dist/browser.js && babili -s --no-comments --compact true dist/browser.js -o dist/browser.js",
    "clean": "rm -rf dist"
  },
  "typings": "dist/jsvm.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daluege/jsvm.git"
  },
  "author": "Filip Dal√ºge <daluege@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/daluege/jsvm/issues"
  },
  "homepage": "https://github.com/daluege/jsvm",
  "devDependencies": {
    "babili": "0.0.7",
    "tape": "^4.6.0",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-config-standard": "^1.5.0",
    "typescript": "^2.0.6",
    "typings": "^1.3.2",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "script-transform": "0.2.2"
  }
}
